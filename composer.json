{
    "name": "axoloth/bolt-simple-entities-crud.git",
    "description": "Simple extension using Doctrine entities.",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Axoloth",
            "email": "contact@axoloth.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "twig/twig": "^2.12 | ^3.0"
    },
    "require-dev": {
        "bolt/core": "^5.1",
        "symplify/easy-coding-standard": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Axoloth\\BoltSimpleEntitiesCrud\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "Axoloth\\BoltSimpleEntitiesCrud\\Extension"
    }
}
